<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapsql">
	<select id="selectplacelist" resultType="vo">
		select * from placelist
	</select>
	<select id="searchplacelist" parameterType="String" resultType="vo">
		SELECT * FROM placelist
		<trim prefix="where">
			<if test="keyword!=null">
				address LIKE '%#{keyword}%'
			</if>
		</trim>
	</select>
</mapper>